	isbn = {978-0-262-54317-0},
	keywords = {intonation,modeling,phonetics,phonology,prosody,speech},
	pages = {287--318},
	publisher = {{MIT Press}},
	title = {The {K}iel {I}ntonation {M}odel},
	year = {2022}}

